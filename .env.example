WITH_SCHEDULER=false
WITH_HORIZON=false #! Needs Redis
WITH_REVERB=false
WITH_WORKER=false
WORKER_COMMAND="php artisan queue:work"

PANEL_ROUTE=admin #! use only minuscules

ONLY_FILAMENT=true
ADMIN_EMAIL=admin@example.com
ADMIN_USER=admin
ADMIN_PASSWORD=admin

DEBUGBAR_ENABLED=false
